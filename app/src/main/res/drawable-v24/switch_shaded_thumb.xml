<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:bottom="6dp" android:end="6dp" android:start="6dp" android:top="6dp">
                <shape android:shape="oval">
                    <gradient android:angle="270.0" android:endColor="@color/settingslib_track_on_color_end" android:startColor="@color/settingslib_track_on_color_start" />
                    <size android:width="14dp" android:height="14dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:bottom="6dp" android:end="6dp" android:start="6dp" android:top="6dp">
                <shape android:shape="oval" android:tint="@color/textColorPrimary">
                    <gradient android:angle="270.0" android:endColor="#33FFFFFF" android:startColor="#66FFFFFF" />
                    <size android:width="12dp" android:height="12dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
